{
  "name": "Mapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "yarn react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android": "yarn react-native run-android",
    "clean": "cd ./android && ./gradlew clean",
    "getpods": "cd ./ios && pod install && cd ..",
    "bundle": "cd ./android && ./gradlew bundle && open ./app/build/outputs/bundle/release/",
    "generate": "cd ./android && ./gradlew app:assembleRelease && open ./app/build/outputs/apk/release/"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-gifted-chat": "^0.11.3",
    "react-native-image-picker": "^1.1.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.26.1",
    "react-native-navigation": "3.2.0-snapshot.524",
    "react-native-vector-icons": "^6.6.0",
    "rn-sliding-up-panel": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
